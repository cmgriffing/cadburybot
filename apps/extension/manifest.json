{
  "name": "Now Playing",
  "version": "1.1.2",
  "description": "Logs currently playing song and album to text file and updates nightbot command.",
  "manifest_version": 3,
  "background": {
    "service_worker": "src/background.ts"
  },
  "action": {
    "default_title": "Now Playing"
  },
  "permissions": ["tabs", "scripting", "activeTab", "alarms"],
  "host_permissions": ["http://*/*", "https://*/*"]
}
